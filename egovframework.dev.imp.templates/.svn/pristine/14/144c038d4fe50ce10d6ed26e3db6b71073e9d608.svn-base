<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    
    <bean name="${txtIdServiceName}" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrService" 
                         destroy-method="destroy">
        <property name="dataSource" ref="${txtDatasourceName}"/>
        <property name="query"  value="${txtQuery}"/>
#if(${rdoType} == "BigDecimal")        
        <property name="useBigDecimals" value="true"/>
#end        
    </bean>  
</beans>  